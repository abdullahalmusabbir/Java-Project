<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="655.0" prefWidth="800.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CheckoutsController">
   <children>
      <VBox alignment="TOP_CENTER" maxWidth="-Infinity" prefWidth="400.0" spacing="24.0">
         <children>
            <VBox alignment="TOP_CENTER" spacing="8.0">
               <children>
                  <TextField fx:id="memberIDField" alignment="CENTER" maxWidth="-Infinity" prefHeight="35.0" prefWidth="250.0" promptText="Member ID">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </TextField>
                  <TextField fx:id="itemIDField" alignment="CENTER" maxWidth="-Infinity" prefHeight="35.0" prefWidth="250.0" promptText="Item ID">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </TextField>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" spacing="5.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#checkSearch" prefHeight="35.0" prefWidth="162.0" text="Search">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#checkin" prefHeight="35.0" prefWidth="162.0" text="Check In">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#checkout" prefHeight="35.0" prefWidth="162.0" text="Check Out">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
            <TextArea fx:id="textarea" prefHeight="302.0" prefWidth="703.0" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
   </padding>
</VBox>
